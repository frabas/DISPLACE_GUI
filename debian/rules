#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

# This has to be exported to make some magic below work.
export DH_OPTIONS

# Override dh_auto_configure to pass the desired CMake options
override_dh_auto_configure:
	dh_auto_configure -- -DWITHOUT_GUI=on

%:
	dh $@ --parallel
